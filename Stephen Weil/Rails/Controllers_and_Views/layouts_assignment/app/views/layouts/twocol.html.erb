<!DOCTYPE html>
<html>
<head>
  <title><%= yield :title  %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <%= yield :table %>
            </div>
            <div class="col-md-6">
                <%= yield :form %>
            </div>
        </div>
    </div>
</body>
</html>
