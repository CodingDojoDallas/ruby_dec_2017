<!DOCTYPE html>
<html lang="en">
<head>
	<title>Posts</title>
	  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	  <%= csrf_meta_tags %>

	  <!-- Dynamically add some Javascript -->
	  <script type="text/javascript">
	      <%= yield :some_javascript %>
	  </script>
</head>
<body>
	<div class="container">
		<div class="row">
			<div class="col-sm-4">
				<h1><%= yield :title %></h1>
				<%= yield :posts %>
			</div>
			<div  class="col-sm-4">
				<%= yield :form %>
			</div>
			<div  class="col-sm-4">
				<%= yield :users %>
			</div>
		</div>
	</div>
	
</body>
</html>

